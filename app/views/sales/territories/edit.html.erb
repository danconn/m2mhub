<% title 'Edit Sales Territory' %>

<%= render 'sales/opportunities/submenu' %>

<h2>Editing Sales Territory</h2>

<%= semantic_form_for @territory, :url => sales_territory_url(@territory) do |f| %>
  <%= render :partial => "form", :locals => {:f => f} %>
	<%= f.buttons do %>
	  <%= f.commit_button %>
	  <%= f.cancel_link %>
	<% end %>
<% end %>
